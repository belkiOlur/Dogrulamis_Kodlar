@using My_ASpNetCoreApp.Web.Controllers
@{
    ViewData["Title"] = "Index";
}

<h1>Ornek Controller, Index Sayfası</h1>

@{
    var name = ViewBag.name;
    <p>@name</p>
}
@model             List<My_ASpNetCoreApp.Web.Controllers.Product>;
@foreach (var item in Model)
{
    <p> @item.Id - @item.Name</p>
}


<p>@name</p>
<p>@ViewBag.name</p>

<hr />
<p>Yaş : @ViewData["age"]</p>
<h1> İsimler</h1>
@foreach (var item in ViewData["names"] as List<string>)
    {
    <p>@item</p>
    }
<p>Şahıs No : @ViewBag.person.Id  </p>
<p>Şahsın Adı : @ViewBag.person.name </p>
<p>Şahsın Yaşı :  @ViewBag.person.age</p>  